<table class="table" id="remain-list-table">
  <BR />
  <col style="width:20%">
  <col style="width:20%">
  <col style="width:40%">
  <thead>
    <tr>
      <th>Title</th>
      <th>Question</th>
      <th>Remain URL</th>
      <!-- <th>Owner</th> -->
    </tr>
  </thead>

    <tbody>
      {{#each remains as |remain|}}
        <tr data-id="{{remain.id}}">
          <td>{{remain.title}}</td>
          <td>{{remain.question}}</td>
          <td>{{remain.id}}</td>
          <td class="hideMe">{{remain._owner}}</td>
          <!-- <td>{{remain.question1}}</td> -->
          <td class="glyphicon"><button type="button" class="remove-remain btn btn-default glyphicon" data-id="{{remain.id}}"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></button></td>
          <td class="glyphicon"><button type="button" class="update-remain btn btn-default glyphicon" data-id="{{remain.id}}"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></button></td>
          <td class="glyphicon"><button type="button" id="{{remain.id}}" class="dashboard-button see-response btn btn-default glyphicon" data-id="{{remain.id}}"><span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span></button></td>

      </tr>
    </tbody>

    {{/each}}

  </table>
